{
  "name": "svelte-babylon",
  "version": "0.40.0",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview --port 3000",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "release": "npm run build && npm run test && npm run package && cd package && npm publish",
    "test": "playwright test"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.8",
    "@playwright/test": "^1.33.0",
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.15.9",
    "@sveltejs/vite-plugin-svelte": "^2.1.1",
    "@svelteness/kit-docs": "^1.1.3",
    "@types/cannon": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "clsx": "^1.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "shiki": "^0.14.1",
    "svelte": "^3.58.0",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.4.14",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unplugin-icons": "^0.16.1",
    "vite": "^4.3.3"
  },
  "dependencies": {
    "@babylonjs/core": "^6.0.0",
    "@babylonjs/gui": "^6.0.0",
    "@babylonjs/inspector": "^6.0.0",
    "@babylonjs/loaders": "^6.0.0",
    "@babylonjs/materials": "^6.0.0",
    "@babylonjs/post-processes": "^6.0.0",
    "@babylonjs/procedural-textures": "^6.0.0",
    "@babylonjs/serializers": "^6.0.0",
    "ammo.js": "^0.0.10",
    "cannon": "^0.6.2",
    "earcut": "^2.2.4"
  },
  "license": "MIT (modified)",
  "type": "module",
  "engines": {
    "node": "^18"
  },
  "engineStrict": true
}
