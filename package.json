{
  "name": "svelte-babylon",
  "version": "0.13.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "release": "npm run package && cd package && npm publish",
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview"
  },
  "devDependencies": {
    "@babylonjs/core": "^5.2.0",
    "@babylonjs/gui": "^5.2.0",
    "@babylonjs/inspector": "^5.2.0",
    "@babylonjs/loaders": "^5.2.0",
    "@babylonjs/materials": "^5.2.0",
    "@babylonjs/post-processes": "^5.2.0",
    "@babylonjs/procedural-textures": "^5.2.0",
    "@babylonjs/serializers": "^5.2.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.36",
    "@types/cannon": "^0.1.8",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vitebook/client": "^0.23.4",
    "@vitebook/core": "^0.23.4",
    "@vitebook/markdown-shiki": "^0.23.4",
    "@vitebook/markdown-svelte": "^0.23.4",
    "@vitebook/theme-default": "^0.23.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "^0.4.14",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^2.8.4"
  },
  "license": "MIT (modified)",
  "type": "module",
  "dependencies": {
    "ammo.js": "^0.0.10",
    "cannon": "^0.6.2",
    "earcut": "^2.2.3"
  }
}
